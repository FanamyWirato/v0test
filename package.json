  "description": "The horse games admin panel",
  "main": "src/WebApp/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_PATH=. nodemon -L --watch src/Server --experimental-modules --es-module-specifier-resolution=node src/Server/index.mjs",
    "eslint": "eslint src/** --ext .mjs",
    "stylelint": "stylelint src/WebApp/**/*.scss",
    "doc": "jsdoc src/Server -c jsdoc.conf.json -R README.md",
    "serve": "vue-cli-service serve src/WebApp/main.js",
    "build": "vue-cli-service build src/WebApp/main.js",
    "lint": "vue-cli-service lint src/WebApp/main.js",
    "watch": "vue-cli-service build --watch --mode development src/WebApp/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cavalcando/AdminPanel.git"
  },
