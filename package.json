      "name": "Florian Maak",
      "email": "florian.maak@posteo.de"
    },
    {
      "name": "Patricia Berger",
      "email": "p.berger+github@posteo.de"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cavalcando/AdminPanel/issues"
  },
  "homepage": "https://github.com/Cavalcando/AdminPanel#readme",
  "dependencies": {
    "@cavalcando/base": "0.0.3",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http": "0.0.1-security",
    "nodemon": "^2.0.2",
    "path": "^0.12.7",
    "socket.io": "^2.3.0",
    "vue": "^3.0.0",
    "vue-socket.io": "^3.0.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "sass": "^1.32.8",
    "sass-loader": "10.1.1",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
